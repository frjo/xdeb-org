{
    "manifest_version": 2,
    "default_locale": "en",
    "lang": "en",
    "name": "xdeb.org",
    "short_name": "xdeb.org",
    "description": "The blog of Fredrik Jonsson, web developer and Linux sysadmin.",
    "icons": [
        {
            "src": "/apple-touch-icon.png",
            "sizes": "180x180",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "/mstile-150x150.png",
            "sizes": "150x150",
            "type": "image/png"
        }
    ],
    "theme_color": "#ffffff",
    "background_color": "#ffffff",
    "start_url": "/index.html",
    "scope": "/",
    "display": "browser",
    "orientation": "any"
}
